      <!-- Sign In Modal -->
      <div class="modal fade" id="signinModal" role="dialog">
         <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
               <div class="modal-header" style="padding:35px 50px;">
                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                  <h4><span class="glyphicon glyphicon-lock"></span> Login</h4>
               </div>
               <div id = "elogin" style="display: none;" class="alert alert-danger"></div>
               <div class="modal-body" style="padding:40px 50px;">
                  <form id="login_form" action = "/login" method="post" role="form">
                     <div class="form-group">
                        <label for="usrname"><span class="glyphicon glyphicon-user"></span> Username</label>
                        <input type="text" required class="form-control" id="username1" name="username" placeholder="Enter username">
                     </div>
                     <div class="form-group">
                        <label for="psw"><span class="glyphicon glyphicon-eye-open"></span> Password</label>
                        <input type="password" required class="form-control" id="password1" name="password" placeholder="Enter password">

                     </div>
                     <div class="checkbox">
                        <label><input id = "remember" type="checkbox" value="" checked>Remember me</label>
                     </div>
                     <button type="submit" class="btn btn-success btn-block"><span class="glyphicon glyphicon-off"></span> Login</button>
                  </form>
               </div>
               <div class="modal-footer">
                  <button type="submit" class="btn btn-danger btn-default pull-left" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> Cancel</button>
                  <p>Not a member? <button onclick="mswitch(2)">Sign Up</button></p>
                  <!--<p>Forgot <a href="#">Password?</a></p>-->
               </div>
            </div>

         </div>
      </div>

      <!-- Sign Up Modal -->
      <div class="modal fade" id="signupModal" role="dialog">
         <div class="modal-dialog">

            <!-- Modal content-->
            <div class="modal-content">
               <div class="modal-header" style="padding:35px 50px;">
                  <button type="button" class="close" data-dismiss="modal">&times;</button>
                  <h4><span class="glyphicon glyphicon-lock"></span> Register</h4>
               </div>
               <div id = "eregister" style="display: none;" class="alert alert-danger"></div>
               <div class="modal-body" style="padding:40px 50px;">
                  <form id= "register_form" action = "/register" method="post" role="form">
                     <div class="form-group">
                        <label for="usrname"><span class="glyphicon glyphicon-user"></span> Username</label>
                        <input type="text" minlength="4" required class="form-control" id="username" name="username" placeholder="Enter Username">
                     </div>
                     <div class="form-group">
                        <label for="psw"><span class="glyphicon glyphicon-eye-open"></span> Password</label>
                        <input type="password" required class="form-control" id="password" name="password" placeholder="Enter password">
                     </div>
                     <div class="form-group">
                        <label for="roll"><span class="glyphicon glyphicon-eye-open"></span> Roll No</label>
                        <input type="text" required class="form-control" id="rollno" name="rollno" placeholder="Enter College Roll No">
                     </div>
                     <div class="form-group">
                        <label for="mob"><span class="glyphicon glyphicon-eye-open"></span> Mobile</label>
                        <input type="number" min="7000000000" max="9999999999" required class="form-control" id= "mobileno" name="mobileno" placeholder="Enter Number">
                     </div>
                     <button type="submit" class="btn btn-success btn-block"><span class="glyphicon glyphicon-off"></span> Register</button>
                  </form>
               </div>
               <div class="modal-footer">
